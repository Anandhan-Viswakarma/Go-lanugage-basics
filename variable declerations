package main

import "fmt"

func main() {

	var v, n int = 5, 6
	f := "short"
	fmt.Println(f)
	a, b := 1, 2
	fmt.Println("\n", a, b)
	fmt.Printf("\n%v", v+v)
	fmt.Println(n)
	fmt.Println("\n", v, n)

	//here its observable that Printf() allows you to give display multiple varible outputs but doesnt give single variable outputs
	//to get a single variable output use Println()
}
